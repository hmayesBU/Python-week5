def square_of_numbers():
    """
    Prompt the user for an integer and print the square of all numbers up to the input number
    eg: input 5 then output (1, 4, 9, 16, 25)
    """

    input_num = int(input("Please enter a number: "))
    square_list = []

    for num in range(1,input_num+1):
        print(num)
        square_list.append(num * num)

    print (square_list)


square_of_numbers()